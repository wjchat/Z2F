        this.tween = TweenLite.to(this.shadowBox, .4,{
            height: '100%',
            width: '100%',
            top:0,
            backgroundColor: 'rgba(255,255,255,1)',
            zIndex: 200,
            borderColor: 'rgba(255,255,255,0)',
            boxShadow: '0 1px 3px rgba(0,0,0,0), 0 1px 2px rgba(0,0,0,0)',
            borderRadius: '0px',
            paddingTop: '4%'
        });